{
    "name": "project-structure-tree",
    "displayName": "Directory Structure Generator",
    "publisher": "FinnClancy",
    "version": "0.0.5",
    "description": "A VS Code extension that creates a project_structure.txt file in the root of your project. The file is a tree of your repo, respecting .gitignore entries.",
    "engines": {
      "vscode": "^1.90.0"
    },
    "icon": "images/icon.png",
    "main": "extension.js",
    "activationEvents": [
      "onCommand:projectStructure.generate"
    ],
    "categories": [
      "Other"
    ],
    "contributes": {
      "commands": [
        {
          "command": "projectStructure.generate",
          "title": "Project Structure"
        }
      ]
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/finnjclancy/project_structure"
    },
    "bugs": {
      "url": "https://github.com/finnjclancy/project_structure/issues"
    },
    "homepage": "https://github.com/finnjclancy/project_structure#readme",
    "license": "ISC"
  }  